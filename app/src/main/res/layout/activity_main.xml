<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.jchuynh99.mywebviewer3.MainActivity">

    <GridLayout
        android:layout_width="356dp"
        android:layout_height="584dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.354"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.333"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:layout_column="0"
            android:layout_row="0"
            android:text="@string/TextViewSelectWebsite"
            android:textColor="@android:color/holo_green_dark"
            android:textSize="30sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="match_parent"
            android:layout_height="@android:dimen/notification_large_icon_height"
            android:layout_column="0"
            android:layout_row="1"
            android:gravity="bottom|center"
            android:text="@string/TextviewUCSDextension"
            android:textColor="?android:attr/colorPrimaryDark"
            android:textSize="30sp"
            android:textStyle="italic" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="match_parent"
            android:layout_height="@android:dimen/notification_large_icon_height"
            android:layout_column="0"
            android:layout_row="3"
            android:gravity="bottom|center"
            android:text="@string/TextViewYahoo"
            android:textColor="?android:attr/colorPrimaryDark"
            android:textSize="30sp"
            android:textStyle="italic" />

        <Button
            android:id="@+id/buttonUcsd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:layout_row="2"
            android:text="@string/buttonforUcsd"
            android:textColor="@android:color/holo_red_dark"
            android:textSize="30sp"
            android:textStyle="italic" />

        <Button
            android:id="@+id/buttonYahoo"
            android:layout_width="match_parent"
            android:layout_height="54dp"
            android:layout_column="0"
            android:layout_row="4"
            android:text="@string/buttonforYahoo"
            android:textColor="@android:color/holo_red_dark"
            android:textSize="30sp"
            android:textStyle="italic" />

    </GridLayout>

</android.support.constraint.ConstraintLayout>
